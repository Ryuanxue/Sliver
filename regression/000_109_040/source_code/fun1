void goodG2B1()
{
    char * data;
    char dataBuffer[100] = "";
    data = dataBuffer;
    switch(5)
    {
    case 6:

        printLine("Benign, fixed string");
        break;
    default:

        strcpy(data, "15");
        break;
    }
    switch(7)
    {
    case 7:
    {
        int i, n, intVariable;
        if (sscanf(data, "%d", &n) == 1)
        {

            intVariable = 0;
